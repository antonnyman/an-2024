package not_found

import "an-2024/views/layout"

templ FiveHundredView() {
	@layout.Base() {
		@layout.Header(true)
		<main class="main">
			<div class="container">
				<div class="content">
					<h1>500: Internal server error</h1>
					<div class="grid-full">
						<p>Something went wrong with the request. <a href="/">Return to start page</a>.</p>
					</div>
				</div>
			</div>
		</main>
	}
}
